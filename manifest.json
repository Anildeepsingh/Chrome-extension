{
    "manifest_version" : 2,
    "name" : "Calculator",
    "version" : "1.0",
    "description" : "Calculate Anywhere",
    "icons" : {
        "128" : "calculator128.png",
        "48" : "calculator48.png",
        "16" : "calculator16.png"
    },
    "browser_action" : {
        "default_icon" : "calculator16.png",
        "default_popup" : "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}